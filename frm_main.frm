Public Function DigiClock(TimeIn As String, DayIn As String, MonthIn As String, YearIn As String)
    
    If Len(TimeIn) = 7 Then
        Temp_digiNum_time(1) = 0
        Temp_digiNum_time(2) = Mid(TimeIn, 1, 1)
        Temp_digiNum_time(3) = Mid(TimeIn, 3, 1)
        Temp_digiNum_time(4) = Mid(TimeIn, 4, 1)
        Temp_digiNum_time(5) = Mid(TimeIn, 6, 1)
        Temp_digiNum_time(6) = Mid(TimeIn, 7, 1)
    ElseIf Len(TimeIn) = 8 Then
        Temp_digiNum_time(1) = Mid(TimeIn, 1, 1)
        Temp_digiNum_time(2) = Mid(TimeIn, 2, 1)
        Temp_digiNum_time(3) = Mid(TimeIn, 4, 1)
        Temp_digiNum_time(4) = Mid(TimeIn, 5, 1)
        Temp_digiNum_time(5) = Mid(TimeIn, 7, 1)
        Temp_digiNum_time(6) = Mid(TimeIn, 8, 1)
    End If
    
    Temp_digiNum_day(1) = Mid(DayIn, 1, 1)
    Temp_digiNum_day(2) = Mid(DayIn, 2, 1)
    
    Temp_digiNum_month(1) = Mid(MonthIn, 1, 1)
    Temp_digiNum_month(2) = Mid(MonthIn, 2, 1)
    
    Temp_digiNum_year(1) = Mid(YearIn, 1, 1)
    Temp_digiNum_year(2) = Mid(YearIn, 2, 1)
    Temp_digiNum_year(3) = Mid(YearIn, 3, 1)
    Temp_digiNum_year(4) = Mid(YearIn, 4, 1)
    
    Call GenerateDigiClock(Temp_digiNum_time(1), Temp_digiNum_time(2), Temp_digiNum_time(3), Temp_digiNum_time(4), Temp_digiNum_time(5), Temp_digiNum_time(6), Temp_digiNum_day(1), Temp_digiNum_day(2), Temp_digiNum_month(1), Temp_digiNum_month(2), Temp_digiNum_year(1), Temp_digiNum_year(2), Temp_digiNum_year(3), Temp_digiNum_year(4))
    
End Function

Public Function GenerateDigiClock(Digi_time_1 As String, Digi_time_2 As String, Digi_time_3 As String, Digi_time_4 As String, Digi_time_5 As String, Digi_time_6 As String, Digi_day_1 As String, Digi_day_2 As String, Digi_month_1 As String, Digi_month_2 As String, Digi_year_1 As String, Digi_year_2 As String, Digi_year_3 As String, Digi_year_4 As String)

    On Error Resume Next

    time_(2).Picture = LoadResPicture("TIME_X", 0)
    
    Counter = 0
    
    If ABTF = True Then
        
        dc(7).Picture = LoadResPicture("TIME_" & Digi_time_1, 0)
        dc(8).Picture = LoadResPicture("TIME_" & Digi_time_2, 0)
        dc(9).Picture = LoadResPicture("TIME_" & Digi_time_3, 0)
        dc(10).Picture = LoadResPicture("TIME_" & Digi_time_4, 0)
        dc(11).Picture = LoadResPicture("TIME_" & Digi_time_5, 0)
        dc(12).Picture = LoadResPicture("TIME_" & Digi_time_6, 0)
        
        dd(3).Picture = LoadResPicture("DATE_" & Digi_day_1, 0)
        dd(4).Picture = LoadResPicture("DATE_" & Digi_day_2, 0)
        
        dd(7).Picture = LoadResPicture("DATE_" & Digi_month_1, 0)
        dd(8).Picture = LoadResPicture("DATE_" & Digi_month_2, 0)
        
        dd(13).Picture = LoadResPicture("DATE_" & Digi_year_1, 0)
        dd(14).Picture = LoadResPicture("DATE_" & Digi_year_2, 0)
        dd(15).Picture = LoadResPicture("DATE_" & Digi_year_3, 0)
        dd(16).Picture = LoadResPicture("DATE_" & Digi_year_4, 0)
        
        Timer2.Enabled = True
        
    ElseIf ABTF = False Then
        
        time_(0).Picture = LoadResPicture("TIME_X", 0)
        time_(1).Picture = LoadResPicture("TIME_X", 0)
        
        dc(1).Picture = LoadResPicture("TIME_" & Digi_time_1, 0)
        dc(2).Picture = LoadResPicture("TIME_" & Digi_time_2, 0)
        dc(3).Picture = LoadResPicture("TIME_" & Digi_time_3, 0)
        dc(4).Picture = LoadResPicture("TIME_" & Digi_time_4, 0)
        dc(5).Picture = LoadResPicture("TIME_" & Digi_time_5, 0)
        dc(6).Picture = LoadResPicture("TIME_" & Digi_time_6, 0)
        
        dd(1).Picture = LoadResPicture("DATE_" & Digi_day_1, 0)
        dd(2).Picture = LoadResPicture("DATE_" & Digi_day_2, 0)
        
        dd(5).Picture = LoadResPicture("DATE_" & Digi_month_1, 0)
        dd(6).Picture = LoadResPicture("DATE_" & Digi_month_2, 0)
        
        dd(9).Picture = LoadResPicture("DATE_" & Digi_year_1, 0)
        dd(10).Picture = LoadResPicture("DATE_" & Digi_year_2, 0)
        dd(11).Picture = LoadResPicture("DATE_" & Digi_year_3, 0)
        dd(12).Picture = LoadResPicture("DATE_" & Digi_year_4, 0)
        
        Timer2.Enabled = False
    
    End If
    
End Function

Private Sub Timer2_Timer()

    On Error Resume Next

    If Counter > 240 Then
        Counter = 240
    End If

    Counter = Counter + Val(1)
    
    With BF
        .BlendOp = AC_SRC_OVER
        .BlendFlags = 0
        .SourceConstantAlpha = Counter
        .AlphaFormat = 0
    End With
    
    RtlMoveMemory lBF, BF, 4
    
    AlphaBlend time_(0).hdc, 0, 0, time_(0).ScaleWidth, time_(0).ScaleHeight, time_(2).hdc, 0, 0, time_(2).ScaleWidth, time_(2).ScaleHeight, lBF
    AlphaBlend time_(1).hdc, 0, 0, time_(1).ScaleWidth, time_(1).ScaleHeight, time_(2).hdc, 0, 0, time_(2).ScaleWidth, time_(2).ScaleHeight, lBF
    
